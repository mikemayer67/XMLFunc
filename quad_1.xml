<?xml  ?>
<!-- Quadratic function using the minus option -->
<arglist>
  <arg name="a" type="double"/>
  <arg name="b"/>
  <arg type="int"></arg>
  <arg name="d" type="integer"/>
</arglist>

<func>
  <div>
    <sub>
      <neg arg="b"/>
      <sqrt>
        <sub>
          <pow arg1="b" arg2='2'/>
          <mult arg1='4'><arg name="a"/><arg index='2'/></mult>
        </sub>
      </sqrt>
    </sub>
    <mult arg2='2' arg1='a'/>
  </div>
</func>

<func name="root2">
  <div>
    <add>
      <neg arg="b"/>
      <sqrt>
        <sub>
          <pow arg1="b" arg2='2'/>
          <mult arg1='4'><arg name="a"/><arg index='2'/></mult>
        </sub>
      </sqrt>
    </add>
    <mult arg2='2' arg1='a'/>
  </div>
</func>
