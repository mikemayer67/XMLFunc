  <arg name="d" type="integer"/>
</arglist>

<div>
  <sub>
    <neg arg="b"/>
    <sqrt>
      <sub>
        <pow  > b  2</pow>
        <mult>4<arg name="a"><arg index=2/>  6  </mult>
      </sub>
    </sqrt>
  </sub>
  <mult>2 a</mult>
</div>
