<!-- Quadratic function using the plus option -->
<arglist>
  <arg name="a" type="double"/>
  <arg name="b"/>
  <arg/>
</arglist>

<div>
  <add>
    <neg arg="b"/>
    <sqrt>
      <sub>
        <pow>b 2</pow>
        <mult>4<arg name="a"><arg index=2/></mult>
      </sub>
    </sqrt>
  </sub>
  <mult>2 a</mult>
</div>
